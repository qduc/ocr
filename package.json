{
  "name": "multi-engine-browser-ocr",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "description": "Multi-Engine Browser OCR System with Tesseract.js and Transformers.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@qduc/easyocr-core": "^0.1.2",
    "@qduc/easyocr-web": "^0.1.2",
    "@xenova/transformers": "^2.17.2",
    "esearch-ocr": "^8.5.0",
    "onnxruntime-web": "^1.23.2",
    "tesseract.js": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "fast-check": "^3.15.1",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-static-copy": "^3.1.4",
    "vitest": "^1.2.1"
  }
}
